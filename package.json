    "lint:fix": "solhint contracts/*.sol --formatter stylish --fix",
    "package:windows": "nexe server/server.js -n PlennyOracle -t windows-x64-10.16.0",
    "package:linux": "nexe server/server.js -n PlennyOracle -t linux-x64-10.16.0",
    "package:mac": "nexe server/server.js -n PlennyOracle -t mac-x64-10.16.0",
    "generate-docs": "solidity-docgen --exclude=./contracts/interfaces -t .",
    "webpack": "node_modules/.bin/webpack"
